{
  "name": "mozaik-theme-starter",
  "description": "WordPress Theme Boilerplate - Build Tools Included :)",
  "version": "4.3.0",
  "author": "Mozaik Ltd.",
  "authorURI": "http://mozaik.com/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MozaikAgency/wp-theme-starter.git"
  },
  "keywords": [
    "mozaik",
    "wordpress",
    "theme",
    "gulp"
  ],
  "scripts": {
    "postinstall": "npm run bower -- install && npm run install:wpcs",
    "install:wpcs": "npm run rimraf -- wpcs && npm run composer -- create-project wp-coding-standards/wpcs:dev-master --no-dev --keep-vcs",
    "test": "npm run lint:scss && npm run lint:js && npm run lint:php",
    "lint:scss": "stylelint ./assets/scss/**/*.scss",
    "lint:js": "eslint ./assets/js/**/*.jsx?",
    "lint:php": "npm run phpcs",
    "dev": "gulp",
    "prod": "gulp build",
    "bower": "bower",
    "composer": "php ./node_modules/getcomposer/composer.phar",
    "phpcs": "./wpcs/vendor/bin/phpcs",
    "rimraf": "rimraf"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "6.24.0",
    "babel-eslint": "7.2.1",
    "babel-loader": "6.4.1",
    "babel-plugin-import-glob": "^1.0.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-stage-2": "6.22.0",
    "bower": "^1.8.0",
    "browser-sync": "2.18.8",
    "css-loader": "^0.28.0",
    "del": "2.2.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-import-resolver-node": "^0.3.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "1.7.1",
    "eslint-plugin-babel": "4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "getcomposer": "^1.3.3",
    "gulp": "3.9.1",
    "gulp-add": "0.0.2",
    "gulp-favicons": "^2.2.7",
    "gulp-filter": "5.0.0",
    "gulp-insert": "0.5.0",
    "gulp-notify": "3.0.0",
    "gulp-plumber": "1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace-task": "^0.11.0",
    "gulp-svg-sprite": "1.3.6",
    "gulp-svgmin": "1.2.3",
    "gulp-util": "3.0.8",
    "gulp-watch": "4.3.11",
    "image-webpack-loader": "^3.3.0",
    "imagemin": "^5.2.2",
    "lodash": "4.17.4",
    "map-stream": "0.0.7",
    "merge-stream": "1.0.1",
    "node-notifier": "5.1.2",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.3.3",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "stylelint": "^7.10.1",
    "stylelint-config-wordpress": "^9.1.1",
    "url-loader": "^0.5.8",
    "vinyl-named": "1.1.0",
    "vinyl-transform": "1.0.0",
    "webpack": "2.3.3",
    "webpack-merge": "^4.1.0",
    "webpack-stream": "3.2.0"
  },
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "lazysizes": "3.0.0",
    "picturefill": "3.0.2"
